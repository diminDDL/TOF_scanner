{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/TOF_GUI",
      "stopAtEntry": false,
      "preLaunchTask": "compile-app",
      "cwd": "${workspaceFolder}/build/",
      "targetArchitecture": "amd64",
      "linux": {
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "logging": { "engineLogging": true }
      }
    }
  ]
}
